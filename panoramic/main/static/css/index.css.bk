table{
    font-family:"times";
    font-size:12px;
    border-collapse:collapse;
}
td{
    border-top-style: none;
    border-right-style: none;
    border-bottom-style: none;
    border-left-style: none;
}
td.tt{ position:relative;
    z-index:24;
    color:#3CA3FF;
    font-weight:bold;
    text-decoration:none;
}
td.tt span.tooltip1{
    display:block;
    position:absolute;
    top:180px; left:-120px;
    padding: 15px 0 0 0;
    width:200px;
    color: #f88888;
    text-align: center;
    filter: alpha(opacity:90);
    KHTMLOpacity: 0.90;
    MozOpacity: 0.90;
    opacity: 0.90;
}
