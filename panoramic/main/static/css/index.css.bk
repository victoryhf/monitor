td.tt span.top{
    display: block;
    padding: 30px 8px 0;
    background: url(./static/images/bubble.jpg) no-repeat top;
}
td.tt span.middle{ /* different middle bg for stretch */
    display: block;
    padding: 0 8px; 
    background: url(./static/images/bubble_filler.jpg) repeat bottom; 
}
td.tt span.bottom{
    display: block;
    padding:3px 8px 10px;
    color: #548912;
    background: url(./static/images/bubble.jpg) no-repeat bottom;
}
